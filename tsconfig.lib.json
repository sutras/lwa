{
  "compilerOptions": {
    "lib": ["ESNext", "DOM"],
    "module": "ESNext",
    "moduleResolution": "bundler",
    "target": "ESNext",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": false,
    "declaration": true,
    "emitDeclarationOnly": true,
    "outDir": "temp",
    "rootDir": "src/lib"
  },
  "include": ["src/lib/main.ts"]
}
