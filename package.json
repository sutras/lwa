{
  "name": "lwa",
  "version": "0.2.3",
  "description": "轻量级动画库",
  "main": "./dist/lwa.cjs",
  "module": "./dist/lwa.mjs",
  "types": "./dist/lwa.d.ts",
  "exports": {
    ".": {
      "types": "./dist/lwa.d.ts",
      "node": {
        "import": {
          "production": "./dist/lwa.mjs",
          "development": "./dist/lwa.mjs",
          "default": "./dist/lwa.mjs"
        },
        "require": {
          "production": "./dist/lwa.cjs",
          "development": "./dist/lwa.cjs",
          "default": "./dist/lwa.cjs"
        }
      },
      "import": "./dist/lwa.mjs",
      "require": "./dist/lwa.cjs"
    },
    "./dist/*": "./dist/*",
    "./package.json": "./package.json"
  },
  "scripts": {
    "dev": "vite",
    "build:docs": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "deploy": "node deploy.mjs",
    "buildDocsAndDeploy": "npm run build:docs && npm run deploy",
    "build:dts": "node build-dts.mjs",
    "build:lib": "node build-lib.mjs && npm run build:dts"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.47.0",
    "@tailwindcss/vite": "^4.1.11",
    "@types/markdown-it": "^14.1.2",
    "@types/markdown-it-container": "^2.0.10",
    "@types/node": "^20.14.9",
    "@vitejs/plugin-vue": "^5.0.5",
    "consola": "^3.2.3",
    "fast-glob": "^3.3.3",
    "gh-pages": "^6.1.1",
    "lodash-es": "^4.17.21",
    "markdown-it": "^14.1.0",
    "markdown-it-async": "^2.2.0",
    "markdown-it-container": "^4.0.0",
    "markdown-it-prism": "^3.0.0",
    "pinia": "^2.1.7",
    "prismjs": "^1.30.0",
    "tailwindcss": "^4.1.11",
    "terser": "^5.31.1",
    "typescript": "^5.5.2",
    "unplugin-vue-markdown": "^0.26.2",
    "vite": "^5.3.1",
    "vite-plugin-svg-icons": "^2.0.1",
    "vitepress-plugin-group-icons": "^1.6.2",
    "vue": "^3.4.29",
    "vue-router": "^4.4.0",
    "vue-tsc": "^2.0.21"
  },
  "files": [
    "dist"
  ],
  "author": "wuzhitao",
  "license": "MIT",
  "homepage": "https://github.com/sutras/lwa",
  "repository": {
    "url": "git+https://github.com/sutras/lwa.git"
  },
  "keywords": [
    "animation",
    "animate",
    "anime",
    "动画"
  ]
}
